{
  "name": "@nivo/legends",
  "description": "legend components for nivo dataviz library",
  "version": "0.33.0",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "dependencies": {
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "@nivo/babel-preset": "0.33.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "cross-env": "^5.0.5",
    "jest": "^21.0.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": ">= 16.2.0 < 17.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --verbose ./tests",
    "test:cover": "jest --verbose --coverage ./tests",
    "build:commonjs": "rm -rf lib && cross-env NODE_ENV=commonjs babel src --out-dir lib",
    "build:commonjs:watch": "npm run build:commonjs -- --watch",
    "build:es": "rm -rf es && cross-env NODE_ENV=es babel src --out-dir es",
    "build:es:watch": "npm run build:es -- --watch",
    "build": "npm run build:commonjs && npm run build:es"
  }
}
